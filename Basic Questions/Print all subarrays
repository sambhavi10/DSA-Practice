
//O(N^3)
int main(){
  
  int n = 4;
  //cin >> n;
  int arr[] = {1,2, 3,4};
//   for(int i=0;i<n;i++){
//       cin >> arr[i];
//   }

    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            int sum=0;
            for(int k=i;k<=j;k++){
                cout << arr[k] << " ";
            }
            cout << endl;
        }
    }
  
}
