//Function to push an integer into the stack.
int arr[5];
int top = -1;//before the 0th index
void push(int item){
         arr[++top] = item;
        
}

//Function to remove an item from top of the stack.
int pop(){
    
    if(top == -1) 
        return -1;
    else{
        return arr[top--];
    }
    
}
